[cache]
type=Disk
base=@@PROJECT_DIR@@/data/tiles/tc_cache/

@@MAPNIK_START@@
[mapnik_@@MAPNIK_NAME@@]
type=Mapnik
mapfile=@@PROJECT_DIR@@/mapnik_@@MAPNIK_NAME@@/osm.xml
spherical_mercator=true
tms_type=google
metaTile=yes
debug=off
@@MAPNIK_END@@

# OpenStreetMap data with Mapnik with Mapserver
[mapserver]
type=WMS
url=http://@@APACHE_SERVER_NAME@@/cgi-bin/mapserv
layers=default
metaTile=yes
#metaSize=15,15
srs=EPSG:900913
extension=png
#extent_type=loose
debug=off
#metaBuffer=10
spherical_mercator=true
tms_type=google
